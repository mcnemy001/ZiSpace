<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomNavTextStyle" parent="TextAppearance.AppCompat.Caption">
        <item name="fontFamily">@font/quicksand_medium</item>
        <item name="android:textSize">12sp</item> <!-- Sesuaikan -->
    </style>


    <style name="InterestChip">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/chip_background</item>
        <item name="android:textColor">@color/text_primary</item>
        <item name="android:textSize">12sp</item>
        <item name="android:fontFamily">@font/quicksand_medium</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:elevation">3dp</item>
    </style>

    <style name="CircleImage" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

</resources>